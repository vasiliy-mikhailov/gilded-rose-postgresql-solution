DECLARE
    have REFCURSOR;
    want REFCURSOR;
    res  TEXT;
BEGIN
    perform public.piggly_branch($PIGGLY$1b4f3633db94ff9d$PIGGLY$);
    OPEN have FOR EXECUTE _query($1);
    OPEN want FOR EXECUTE _query($2);
    res := results_ne(have, want, $3);
    CLOSE have;
    CLOSE want;
    perform public.piggly_branch($PIGGLY$4a6be7678f39d36f$PIGGLY$);RETURN res;
