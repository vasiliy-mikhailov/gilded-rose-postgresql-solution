DECLARE
    have REFCURSOR;
    want REFCURSOR;
    res  TEXT;
BEGIN
    perform public.piggly_branch($PIGGLY$400c9e578b7eb952$PIGGLY$);
    OPEN have FOR EXECUTE _query($1);
    OPEN want FOR EXECUTE _query($2);
    res := results_ne(have, want, $3);
    CLOSE have;
    CLOSE want;
    perform public.piggly_branch($PIGGLY$15cd841adc09ecdf$PIGGLY$);RETURN res;
