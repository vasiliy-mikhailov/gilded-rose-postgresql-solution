DECLARE
    have REFCURSOR;
    want REFCURSOR;
    res  TEXT;
BEGIN
    perform public.piggly_branch($PIGGLY$d4c8395201b7bdcd$PIGGLY$);
    OPEN have FOR EXECUTE _query($1);
    OPEN want FOR SELECT $2[i]
    FROM generate_series(array_lower($2, 1), array_upper($2, 1)) s(i);
    res := results_ne(have, want, $3);
    CLOSE have;
    CLOSE want;
    perform public.piggly_branch($PIGGLY$39691b021d1db013$PIGGLY$);RETURN res;
END;