DECLARE
    aname name;
BEGIN
    perform public.piggly_branch($PIGGLY$a67317dcea9e7711$PIGGLY$);
    SELECT am.amname
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ci ON ci.oid = x.indexrelid
      JOIN pg_catalog.pg_am am    ON ci.relam = am.oid
     WHERE ci.relname = $1
      INTO aname;

      perform public.piggly_branch($PIGGLY$32ee75608b998948$PIGGLY$);return is(
          aname, $2,
          'Index ' || quote_ident($1) || ' should be a ' || quote_ident($2) || ' index'
      );
END;