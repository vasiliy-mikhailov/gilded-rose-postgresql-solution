DECLARE
    res boolean;
BEGIN
    perform public.piggly_branch($PIGGLY$8255027b2ab10660$PIGGLY$);
    SELECT x.indisclustered
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ct    ON ct.oid = x.indrelid
      JOIN pg_catalog.pg_class ci    ON ci.oid = x.indexrelid
      JOIN pg_catalog.pg_namespace n ON n.oid = ct.relnamespace
     WHERE ct.relname = $2
       AND ci.relname = $3
       AND n.nspname  = $1
      INTO res;

      perform public.piggly_branch($PIGGLY$2f061ab26e866a8e$PIGGLY$);RETURN ok( COALESCE(res, false), $4 );
END;