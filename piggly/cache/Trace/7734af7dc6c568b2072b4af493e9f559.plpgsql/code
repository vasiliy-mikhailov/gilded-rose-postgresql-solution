DECLARE
    have       ALIAS FOR $1;
    want       ALIAS FOR $2;
    have_rec   RECORD;
    want_rec   RECORD;
    have_found BOOLEAN;
    want_found BOOLEAN;
    err_msg    text := 'details not available in pg <= 9.1';
BEGIN
    perform public.piggly_branch($PIGGLY$7a03e26d90b98040$PIGGLY$);
    FETCH have INTO have_rec;
    have_found := FOUND;
    FETCH want INTO want_rec;
    want_found := FOUND;
    WHILE public.piggly_cond($PIGGLY$98c9f3b1ace0eeb7$PIGGLY$, (have_found OR want_found)) LOOP
        perform public.piggly_branch($PIGGLY$3a87aa642eb5c59d$PIGGLY$);
        IF public.piggly_cond($PIGGLY$2040018b46e2a50a$PIGGLY$, (have_rec IS DISTINCT FROM want_rec OR have_found <> want_found)) THEN
            perform public.piggly_branch($PIGGLY$8e24a81b5b392072$PIGGLY$);
            perform public.piggly_branch($PIGGLY$2c7ed2a920f07b11$PIGGLY$);RETURN ok( true, $3 );
        ELSE
            perform public.piggly_branch($PIGGLY$9af3d49372470c90$PIGGLY$);
            FETCH have INTO have_rec;
            have_found := FOUND;
            FETCH want INTO want_rec;
            want_found := FOUND;
        END IF;
    END LOOP;
    perform public.piggly_branch($PIGGLY$75a5dc7df4062bc6$PIGGLY$);RETURN ok( false, $3 );
EXCEPTION
    WHEN datatype_mismatch THEN
        perform public.piggly_branch($PIGGLY$47533b4f4ed45610$PIGGLY$);
        GET STACKED DIAGNOSTICS err_msg = MESSAGE_TEXT;
        perform public.piggly_branch($PIGGLY$24f206df90207cd0$PIGGLY$);RETURN ok( false, $3 ) || E'\n' || diag(
            E'    Number of columns or their types differ between the queries' ||
            CASE WHEN have_rec::TEXT = want_rec::text THEN '' ELSE E':\n' ||
                '        have: ' || CASE WHEN have_found THEN have_rec::text ELSE 'NULL' END || E'\n' ||
                '        want: ' || CASE WHEN want_found THEN want_rec::text ELSE 'NULL' END
            END || E'\n        ERROR: ' || err_msg
        );
END;