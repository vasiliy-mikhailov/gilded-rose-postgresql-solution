DECLARE
    res boolean;
BEGIN
    perform public.piggly_branch($PIGGLY$deec08c757053980$PIGGLY$);
    SELECT x.indpred IS NOT NULL
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ct ON ct.oid = x.indrelid
      JOIN pg_catalog.pg_class ci ON ci.oid = x.indexrelid
     WHERE ct.relname = $1
       AND ci.relname = $2
       AND pg_catalog.pg_table_is_visible(ct.oid)
     INTO res;

      perform public.piggly_branch($PIGGLY$32a61f90aeeec37a$PIGGLY$);RETURN ok(
          COALESCE(res, false),
          'Index ' || quote_ident($2) || ' should be partial'
      );
END;