DECLARE
    res boolean;
BEGIN
    perform public.piggly_branch($PIGGLY$19db6fab8d551e97$PIGGLY$);
    SELECT x.indisunique
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ct    ON ct.oid = x.indrelid
      JOIN pg_catalog.pg_class ci    ON ci.oid = x.indexrelid
      JOIN pg_catalog.pg_namespace n ON n.oid = ct.relnamespace
     WHERE ct.relname = $2
       AND ci.relname = $3
       AND n.nspname  = $1
      INTO res;

      perform public.piggly_branch($PIGGLY$23a3415d0a196257$PIGGLY$);RETURN ok( COALESCE(res, false), $4 );
END;