DECLARE
    res boolean;
BEGIN
    perform public.piggly_branch($PIGGLY$51d255d08499e547$PIGGLY$);
    SELECT x.indisclustered
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ci ON ci.oid = x.indexrelid
     WHERE ci.relname = $1
      INTO res;

      perform public.piggly_branch($PIGGLY$1dfa9f0e00b12ee0$PIGGLY$);RETURN ok(
          COALESCE(res, false),
          'Table should be clustered on index ' || quote_ident($1)
      );
END;