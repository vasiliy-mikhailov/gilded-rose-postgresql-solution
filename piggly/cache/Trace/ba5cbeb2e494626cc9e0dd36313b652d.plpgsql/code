DECLARE
    res boolean;
BEGIN
    perform public.piggly_branch($PIGGLY$7e528f4c6035efa6$PIGGLY$);
    SELECT x.indisclustered
      FROM pg_catalog.pg_index x
      JOIN pg_catalog.pg_class ci ON ci.oid = x.indexrelid
     WHERE ci.relname = $1
      INTO res;

      perform public.piggly_branch($PIGGLY$8bb8b5b1443a298d$PIGGLY$);RETURN ok(
          COALESCE(res, false),
          'Table should be clustered on index ' || quote_ident($1)
      );
END;