DECLARE
    pname text;
BEGIN
    perform public.piggly_branch($PIGGLY$8b4ab1515f3b94cd$PIGGLY$);
    SELECT p.proname
      FROM pg_catalog.pg_trigger t
      JOIN pg_catalog.pg_class ct ON ct.oid = t.tgrelid
      JOIN pg_catalog.pg_proc p   ON p.oid = t.tgfoid
     WHERE ct.relname = $1
       AND t.tgname   = $2
       AND pg_catalog.pg_table_is_visible(ct.oid)
      INTO pname;

    perform public.piggly_branch($PIGGLY$f2637a9bfcc4e523$PIGGLY$);RETURN is( pname, $3::text, $4 );
END;